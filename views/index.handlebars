<h1 class=text-center mb-3>{{{title}}}</h1>
<form action="/api/finstocks" method="POST" class="mb-4">
    <div class="form-group">
        <label for="name">Name</label>
        <input type="text" name="name" class="form-control">
    </div>
    <div class="form-group">
        <label for="price">Price</label>
        <input type="number" name="price" class="form-control">
    </div>
    <input type="submit" value="Add Finstock" class="btn btn-primary btn-block">
</form>
<h4>finStocks</h4>
<ul class="list-group">
    {{#each finstocks}}
        <li class="list-group-item">{{this.name}}: {{this.price}}</li>
    {{/each}}
</ul>
<a href="/api/members" class="btn btn-light mt-4">Visit API</a>